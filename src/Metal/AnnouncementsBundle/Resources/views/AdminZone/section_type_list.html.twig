{% extends 'SonataAdminBundle:CRUD:base_list_field.html.twig' %}

{% block field %}
    <div>
        {% if object.zone %}
            <div>
                {{ object.zone.title }} - {{ object.zone.section.title }} ({{ object.zone.width }}x{{ object.zone.height }})
            </div>
        {% endif %}
    </div>
{% endblock %}