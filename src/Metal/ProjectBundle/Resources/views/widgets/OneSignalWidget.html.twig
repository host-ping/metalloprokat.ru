<link rel="manifest" href="/manifest.json" />
<script src="https://cdn.onesignal.com/sdks/OneSignalSDK.js" async=""></script>
<script>
    var OneSignal = window.OneSignal || []; OneSignal.push(function() {
        OneSignal.init({ appId: "{{ currentCity.onesignalCode }}",
        });
        OneSignal.sendTags({
            userId: '{{ app.user ? app.user.id : '' }}',
            cityId : '{{ currentCity ? currentCity.id : '' }}',
            regionId : '{{ currentCity ? currentCity.administrativeCenter.id : '' }}',
            category : '{{ currentCategory ? currentCategory.id : '' }}',
            parentCategory : '{{ currentCategory ? ( currentCategory.parent ? currentCategory.parent.id : '') : ''}}'
        });
    });
</script>