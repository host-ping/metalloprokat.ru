{% extends '@markup/private/statistics/_statistic_layout.html.twig' %}
{% block title 'Работа с потребностями' %}
{% block content %}
    <div id="content" class="statistic-content private-room-content content-right outline-right float-right">
        {% block tabs %}
            {% set activeTab = 'stats-demand' %}
            {{ parent() }}
        {% endblock tabs %}
        {% block filters %}
            {{ parent() }}
        {% endblock filters %}

        <div class="demand-tab">
            <div class="income-graph-wrapper">
                <img src="./markup/pic/graph.png" alt="image description"/>
            </div>
            <table class="graph-head stat-table">
                <tr class="row"> {#fixed#}
                    <td class="date col"><span class="head-title sorted desc">Дата</span></td>
                    <td class="view-demands col"><span class="head-title">Просмотры заявок</span></td>
                    <td class="add-favorites col"><span class="head-title sorted asc">Добавлены в избранное</span></td>
                    <td class="answers col"><span class="head-title">Ответы</span></td>
                </tr>
            </table>
            <div class="scrollable-table content-scrollable js-scrollable">
                <table class="graph-info stat-table">
                    <tr class="total row">
                        <td class="date col">Всего</td>
                        <td class="view-demands col">2145</td>
                        <td class="add-favorites col">2</td>
